{
  "files.exclude": {
    //in workspace settings
    "**/node_modules": true,
    "**/*.spec.ts": true,
    "e2e": true,
    ".editorconfig": true,
    "**/*.conf.js": true,
    "package-lock.json": true,
    "protractor.conf.js": true,
    "**.gitignore": true,
    "**/*.spec.json": true,
    "**/test.ts": true,
    ".prettierignore": true,
    ".prettierrc": false,
    "typings": true,
    "**/*.map": true,
    ".vscode": false,
    "README.md": false,
    "**/*.js": {
      "when": "$(basename).ts"
    }
  }
}